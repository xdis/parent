  <!-- 根据案件id和主流程查询出计划时间 -->
  <select id="selectPlanEndTimeByCaseId" resultType="java.util.Date">
  	SELECT
		cno.plan_end_time planEndTime
	FROM
		case_info ci,
		case_node cn,
		case_node_oper cno
	WHERE
		ci.current_main_node = cn.id
	AND 
		cn.id = cno.node_id
	AND 
		ci.id=cno.case_id
	AND 
		ci.id = #{caseId}
	AND 
		cno.node_id = #{currentMainNode}
  </select>
