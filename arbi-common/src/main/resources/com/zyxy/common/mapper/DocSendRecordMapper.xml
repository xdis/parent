  <!--手动添加的方法-->

    <!-- 仲裁委查询文书出具人 -->
    <select id="arbitrationQueryDocIssuer" resultType="java.lang.String">
    SELECT
      CASE
	      WHEN creater_id IS NULL THEN '0'
	      WHEN creater_id IN (SELECT manager_id FROM case_manager) THEN '0'
	      WHEN creater_id IN (SELECT arbitrator_id FROM case_arbitrator)THEN '1'
	      ELSE '-1'
      END
    FROM
      case_doc
    WHERE
      busi_id=#{busi_id}
      AND
      status = ${@com.zyxy.common.enums.CaseDocStatusEnum@NORMAL.getCode()}
    </select>

