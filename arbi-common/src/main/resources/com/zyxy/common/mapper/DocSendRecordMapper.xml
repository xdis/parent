  <!-- 通过请求id查询案件初次受理时间  -->
  <select id="selectAcceptTimeByRequestId" resultType="java.lang.String">
    	SELECT
    		dsr.create_time
    	FROM
    		doc_send_record dsr
    		LEFT JOIN
    			case_doc cd
    			ON
    				cd.id = dsr.doc_id
    	WHERE
    		cd.request_id = #{requestId}
    		AND
    		cd.doc_type = ${com.zyxy.common.enums.CaseDocTypeEnum@CASE_ACCEPT_NOTICE.getCode()}
    		AND
    		cd.status = ${com.zyxy.common.enums.CaseDocStatusEnum@NORMAL.getCode()}
    	ORDER BY
    		cd.create_time 
    	LIMIT 1
    </select>
