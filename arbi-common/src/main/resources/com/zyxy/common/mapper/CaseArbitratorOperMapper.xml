  <!-- 根据案件ID查询当事人选择的仲裁员 -->
  <select id="selectArbitratorForLitigantByCaseId" resultType="com.zyxy.common.vo.LitigantArbitratorInfoVO">
  	SELECT
		u.name name,
		YEAR ( FROM_DAYS( DATEDIFF( now( ), a.birthday))) age,
		a.vocation job,
		a.specialty specialty,
		c.type type,
		a.head_photo portrait
	FROM
		case_arbitrator_oper c, 
		user_info u,
		arbitrator_info a
	WHERE
		c.arbitrator_id = a.user_id
	AND
		a.user_id = u.id
	AND
		c.type in (${@com.zyxy.common.enums.ArbiChoiceRecordTypeEnum@PROPOSER_CHOICE.getValue()},${@com.zyxy.common.enums.ArbiChoiceRecordTypeEnum@DEFENDANT_CHOICE.getValue()})
	AND	
		c.case_id = #{caseId}  
  </select>
