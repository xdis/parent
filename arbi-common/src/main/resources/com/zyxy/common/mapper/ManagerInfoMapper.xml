  <!--数据统计-->
  <select id="queryDataStatistics" parameterType="com.zyxy.common.dto.DataStatisticsDTO" resultType="com.zyxy.common.domain.DataStatistics">
      SELECT
        COUNT(CASE WHEN ca.approve_status='0' THEN 1 END) notAudited,
        COUNT(CASE WHEN ca.approve_status='1' THEN 1 END) auditAdopt,
        COUNT(CASE WHEN ca.approve_status='2' THEN 1 END) auditNotPass,
        ((COUNT(CASE WHEN ca.approve_status='0' THEN 1 END)) + (COUNT(CASE WHEN ca.approve_status='1' THEN 1 END))) stayAndNotAuditTotal,
        COUNT(1) total
      FROM
        case_info ci,
        case_approve ca
      <if test="userId != null and userId != ''">
        ,case_manager cm,
        user_info ui
      </if>
      WHERE
        ci.id=ca.case_id
    <if test="userId != null and userId != ''">
      AND
        ca.case_id=cm.case_id
      AND
        cm.manager_id=ui.id
      AND
        ui.id=#{userId,jdbcType=VARCHAR}
        <if test="registered != null and registered != ''">
        	AND
        	  ca.approve_type= #{registered}
        </if>
        <if test="number != null and number != ''">
        	AND
        	  ca.approve_type= #{number}
        </if>
    </if>
    <if test="toDay !=null and toDay !=''">
      AND
        TO_DAYS(ca.create_time)=TO_DAYS(#{toDay,jdbcType=VARCHAR})
        <if test="isCancel != null and isCancel != '' || antiCancel != null and antiCancel != ''">
        	AND
        	  ca.approve_type= #{isCancel}
        	OR 
        	  ca.approve_type= #{antiCancel}
        </if>
        <if test="conciliation != null and conciliation != ''">
        	AND
        	  ca.approve_type= #{conciliation}
        </if>
    </if>
    <if test="sameMonth !=null and sameMonth !=''">
      AND
        date_format(ca.create_time,'%Y-%m')=#{sameMonth,jdbcType=VARCHAR}
        <if test="registered != null and registered != ''">
	      AND
	        ca.approve_type= #{registered}
	    </if>
	    <if test="number != null and number != ''">
	      AND
	        ca.approve_type= #{number}
	    </if>
        <if test="counterclaim != null and counterclaim != ''">
	      AND
	        ca.approve_type= #{counterclaim}
	    </if>
	    <if test="change != null and change != ''">
	      AND
	        ca.approve_type= #{change}
	    </if>
	    <if test="avoid != null and avoid != ''">
	      AND
	        ca.approve_type= #{avoid}
	    </if>
    </if>
    <if test="sameMonth ==''">
      AND
        date_format(ca.create_time,'%Y-%m')=#{sameMonth,jdbcType=VARCHAR}
    </if>
  </select>
  
